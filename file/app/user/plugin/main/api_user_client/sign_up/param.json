				"identical": "password"
			}
		},
		{
			"name": "phone",
			"title": "手机号码",
			"type": "string",
			"string": {
				"format": "phone"
			}
		},
		{
			"name": "code",
			"title": "短信验证码",
			"type": "string"
		},
		{
			"name": "invite_code",
			"title": "邀请码",
			"type": "string"
		}
	]
